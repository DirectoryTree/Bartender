{
    "name": "directorytree/bartender",
    "description": "An opinionated way to authenticate users using Laravel Socialite.",
    "keywords": [
        "oauth"
    ],
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
    "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "laravel/socialite": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.34"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "DirectoryTree\\Bartender\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DirectoryTree\\Bartender\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BartenderServiceProvider"
            ],
            "aliases": {
                "Bartender": "DirectoryTree\\Bartender\\Facades\\Bartender"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
